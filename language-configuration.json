{
  "comments": {
    "lineComment": "'",
    "blockComment": ["{", "}"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "\"", "close": "\"", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(If\\b|While\\b|For\\b|Try\\b).*",
    "decreaseIndentPattern": "^\\s*(EndIf\\b|EndWhile\\b|Next\\b|EndTry\\b).*"
  }
}
